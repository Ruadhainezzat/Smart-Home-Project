#include "STD_TYPES.h"
#include "ACTUATOR_config.h"
#ifndef ACTUATOR_INTERFACE_H_
#define ACTUATOR_INTERFACE_H_


typedef struct{
	u8 actuatorPort;
	u8 actuatorChannel;
}Actuator_ChannelConfig;

typedef struct{
	Actuator_ChannelConfig Actuators[NUM_ACTUATORS_CONFIG];
}Actuator_ConfigType;
#endif /* ACTUATOR_INTERFACE_H_ */
